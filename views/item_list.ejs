<!DOCTYPE html>
<html lang="en">
<%- include('templates/head') %>
<body>
   <%- include('templates/navbar') %>
   <% item_list.forEach(item => { %>
      <div class="item-container">
         <h2><%= item.name %></h2>
         <p><a href=<%= item.category.url %>><%= item.category.name %></a></p>
         <p><%= item.price %></p>
      </div>
   <% }); %>
</body>
</html>