<!DOCTYPE html>
<html lang="en">
<%- include('templates/head') %>
<body>
   <%- include('templates/navbar') %>
   <% item_list.forEach(item => { %>
      <div class="item-container">
         <h2><a href=<%= item.url %>><%= item.name %></a></h2>
         <p><a href=<%= item.category.url %>><%= item.category.name %></a></p>
         <p>$<%= item.price %></p>
         <% if (item.number_in_stock <= 20) { %>
            <p class="num-in-stock">Only <%= item.number_in_stock%> left in stock</p>
         <% } %>
      </div>
   <% }); %>
</body>
</html>